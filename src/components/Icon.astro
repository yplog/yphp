---
interface Props {
  alt: string;
  width: string;
  height: string;
  resource: string;
  color?: string;
}

const {
  alt,
  width,
  height,
  resource,
  color= "#d4d4d6"
} = Astro.props;
---

<img style={`fill: ${color}; color: ${color};`}  src={resource} alt={alt} height={height} width={width} color={color} />
